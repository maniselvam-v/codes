class Solution:
    def divide(self, dividend: int, divisor: int) -> int:
        if dividend == -2**31 and divisor == -1:
            return 2**31 - 1
        sign = 1 if (dividend > 0) == (divisor > 0) else -1
        n, d, res = abs(dividend), abs(divisor), 0
        while n >= d:
            temp, mult = d, 1
            while n >= (temp << 1):
                temp <<= 1
                mult <<= 1
            n -= temp
            res += mult
        return sign * res

class Solution(object):
    def twoSum(self, nums, target):
        s=0
        for i in range(len(nums)):
            s=0
            for j in range(len(nums)):
                if i==j:
                    continue
                s=nums[i]+nums[j]
                if s==target:
                    return [i,j]
                else:
                    pass



            
        

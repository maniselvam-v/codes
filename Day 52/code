import collections
class Solution:
    def removeDuplicateLetters(self, s: str) -> str:
        counter=collections.Counter(s)
        stack=[]
        seen=set()
        for c in s:
            counter[c]-=1
            if c in seen:
                continue
            while stack and c<stack[-1] and counter[stack[-1]]:

                seen.remove(stack.pop())
            stack.append(c)
            seen.add(c)
        return ''.join(stack)
